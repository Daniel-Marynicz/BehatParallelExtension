{
  "name": "dmarynicz/behat-parallel-extension",
  "description": "Parallel extension for Behat",
  "keywords": ["BDD", "Behat", "Symfony2", "Context", "Extension"],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=5.6",
    "behat/behat": "^3.6",
    "symfony/process": "^2.7.52 || ^3.0 || ^4.0 || ^5.0.8",
    "symfony/console": "^2.7.52 || ^2.8.33 || ^3.3.15 || ^3.4.3 || ^4.0 || ^5.0",
    "symfony/serializer": "^2.7.52 || ^3.0 || ^4.0 || ^5.0",
    "symfony/dependency-injection": "^2.7.52 || ^3.0 || ^4.0 || ^4.1.12 || ^5.0",
    "symfony/event-dispatcher": "^2.7.52 || ^3.0 || ^4.0 || ^5.0",
    "symfony/yaml": "^2.7.52 || ^3.0 || ^4.0 || ^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0 || ^6.0 || ^7.0",
    "ext-json": "*",
    "dg/bypass-finals": "^1.1 || ^1.2",
    "phpstan/phpstan": "^0.12.23",
    "squizlabs/php_codesniffer": "^3.5",
    "phpstan/phpstan-phpunit": "^0.12.8",
    "doctrine/coding-standard": "^7.0",
    "phpstan/phpstan-symfony": "^0.12.6",
    "phpstan/phpstan-doctrine": "^0.12.12"
  },
  "autoload": {
    "psr-4": {
      "DMarynicz\\BehatParallelExtension\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DMarynicz\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "phpcs": [
      "./vendor/bin/phpcs"
    ],
    "phpcbf": [
      "./vendor/bin/phpcbf"
    ],
    "phpcbfWithoutErrorCode": [
      "./vendor/bin/phpcbf || true"
    ],
    "phpstan": [
      "./vendor/bin/phpstan analyse"
    ],
    "phpunit": [
      "./vendor/bin/phpunit --fail-on-warning "
    ],
    "behat": [
      "./vendor/bin/behat --colors --strict --stop-on-failure"
    ],
    "tests": [
      "@phpcbfWithoutErrorCode",
      "@phpcs",
      "@phpstan",
      "@phpunit",
      "@behat"
    ]
  }
}
