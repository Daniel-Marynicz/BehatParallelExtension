{
  "name": "dmarynicz/behat-parallel-extension",
  "description": "Parallel extension for Behat",
  "keywords": ["BDD", "Behat", "Symfony2", "Context", "Extension"],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=5.4",
    "behat/behat": "^3.0",
    "symfony/process": "^2.7.51 || ^3.0 || ^4.0 || ^5.0",
    "symfony/console": "^2.7.51 || ^3.0 || ^4.0 || ^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8.36 || ^6.0"
  },
  "autoload": {
    "psr-4": {
      "DMarynicz\\BehatParallelExtension\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DMarynicz\\Tests\\": "tests/"
    }
  }
}
